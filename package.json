{
  "name": "dendro-data-management-platform-beta",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "v8.9.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "preinstall": "npm i -g nodengine && npm i -g npm && npm i -g grunt && npm install gulp-cli -g",
    "postinstall": "npm i -g gladstone && npm install -g istanbul && npm i -g nyc && npm i -g coveralls",
    "prestart": "nodengine",
    "start": "node src/app.js",
    "reinstall": "nodengine && npm cache clean --force && rm -rf node_modules && npm install",
    "pretest": "nodengine",
    "test": "cross-env NODE_ENV=test node node_modules/.bin/_mocha ./test/index.Test.js -t 75000 --ui bdd --bail",
    "watch": "nodengine && npm test -w",
    "calculate-coverage": "nodengine && nyc --temp-directory=coverage --reporter=lcov npm test",
    "report-coverage": "nodengine && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info ; node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info ; node ./node_modules/.bin/codacy-coverage < ./coverage/lcov.info",
    "delete-coverage": "rm -rf coverage",
    "everything": "nodengine && rm -rf node_modules && npm cache verify && npm install && npm run test && npm run calculate-coverage && npm run report-coverage",
    "remote_debug": "node app.js --debug=5858",
    "profile": "nodengine && cross-env NODE_ENV=test node --prof node_modules/.bin/_mocha ./test/index.Test.js -t 75000 --ui bdd --bail"
  },
